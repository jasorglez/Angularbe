{
  "rules": {
    "storage": {
      "$file": {
        "read": true,
        "write": false
      }
    }
  },
  "hosting": {
    "public": "dist/app-be",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
